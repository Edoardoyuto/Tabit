{
    "manifest_version": 3,
    "name": "Tab Time Tracker",
    "version": "1.0",
    "permissions": ["tabs", "storage", "tabGroups"],
    "background": {
      "service_worker": "background.js",
      "default_icon": "icon.png"
    },
    "action": {
      "default_popup": "dashboard.html"
    },
     "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
}
