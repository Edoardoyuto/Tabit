{
  "manifest_version": 3,
  "name": "Tab Time Tracker",
  "version": "1.2",
  "permissions": ["tabs", "storage", "tabGroups"],
  "host_permissions": ["<all_urls>"], 
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "dashboard.html",
      "default_icon": "icon.png"
  },
  "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  }
}
